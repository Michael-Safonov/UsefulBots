{
  "MicrosoftAppId": "",
  "MicrosoftAppPassword": "",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "rollingInterval":  "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}